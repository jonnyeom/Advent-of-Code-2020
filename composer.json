{
    "name": "unleashedtech/advent-of-code-2020",
    "description": "Template repository to get up-and-running quickly with Advent of Code",
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "Colin O'Dell",
            "email": "codell@unleashed-technologies.com"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4"
    },
    "autoload": {
        "psr-4": {
            "UnleashedTech\\AdventOfCode2020\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UnleashedTech\\AdventOfCode2020\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
